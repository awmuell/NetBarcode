<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageProjectUrl>https://github.com/awmuell/NetBarcode</PackageProjectUrl>
    <PackageTags>barcode</PackageTags>
    <PackageIconUrl>https://i.imgur.com/BhcO3DF.jpg</PackageIconUrl>
    <PackageLicenseUrl>https://github.com/awmuell/NetBarcode/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/awmuell/NetBarcode</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Filipe Tagliatti, Andrew Mueller</Authors>
    <Company />
    <Product />
    <Description>Barcode generation library written in .NET Core compatible with .NET Standard 2. With Code39E and Type/Size method.</Description>
    <Version>1.0.8</Version>
    <PackageId>awmuell.NetBarcode</PackageId>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard2.0\NetBarcode.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard2.0\NetBarcode.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System.Drawing.Common, Version=4.0.0.0, Culture=neutral, PublicKeyToken=cc7b13ffcd2ddd51">
      <HintPath>..\..\..\..\.nuget\packages\system.drawing.common\4.5.0\ref\netstandard2.0\System.Drawing.Common.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="4.5.0" />
  </ItemGroup>
</Project>